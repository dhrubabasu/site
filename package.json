		"@tailwindcss/typography": "0.5.13",
		"@types/eslint": "9.6.0",
		"autoprefixer": "10.4.20",
		"cssnano": "7.0.4",
		"eslint": "9.8.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.43.0",
		"globals": "15.9.0",
		"postcss": "8.4.41",
		"postcss-cli": "11.0.0",
		"prettier": "3.3.3",
		"prettier-plugin-svelte": "3.2.6",
		"rustywind": "0.22.0",
		"svelte": "5.0.0-next.210",
		"svelte-check": "3.8.5",
		"tailwindcss": "3.4.7",
		"tslib": "2.6.3",
